{
  "manual_call_templates": [
    {
      "name": "filesystem",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "filesystem": {
            "command": "npx",
            "args": [
              "-y",
              "@modelcontextprotocol/server-filesystem",
              "/Users/yourname"
            ]
          }
        }
      }
    },
    {
      "name": "git",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "git": {
            "command": "npx",
            "args": [
              "-y",
              "@modelcontextprotocol/server-git",
              "/Users/yourname/repo/brain"
            ]
          }
        }
      }
    },
    {
      "name": "github",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "github": {
            "command": "/usr/local/bin/github-mcp-server",
            "args": [
              "stdio"
            ],
            "env": {
              "GITHUB_PERSONAL_ACCESS_TOKEN": "set-me"
            }
          }
        }
      }
    },
    {
      "name": "eventkit",
      "call_template_type": "mcp",
      "config": {
        "mcpServers": {
          "eventkit": {
            "url": "http://host.docker.internal:7411/rpc",
            "headers": {
              "Authorization": "Bearer REPLACE_ME"
            }
          }
        }
      }
    }
  ]
}
